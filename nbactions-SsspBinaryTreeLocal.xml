                <goal>org.codehaus.mojo:exec-maven-plugin:1.2:exec</goal>
            </goals>
            <properties>
                <exec.args>${profiler.args} -Xmx3500m -Xms3500m -Djava.security.policy=policy -server -XX:+UseConcMarkSweepGC -XX:-UseParNewGC -verbosegc -classpath %classpath clients.SsspBinaryTree examples/SsspBinaryTree 4</exec.args>
                <exec.executable>${profiler.java}</exec.executable>
                <exec.workingdir>/Users/cappello/NetBeansProjects/munger/jpregel-aws</exec.workingdir>
            </properties>
        </action>
    </actions>
